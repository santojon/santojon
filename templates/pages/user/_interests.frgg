<section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="interests">
    <div class="w-100">
        <h2 class="mb-5">__('Interests')</h2>
        <ul class="fa-ul mb-0">
            {% for interest in user.interests =>
            <li>
                <p>
                    <i class="fa-li fa fa-check"></i>
                    __('{{ interest }}')
                </p>
            </li>
            %}
        </ul>
    </div>
</section>
<hr class="m-0">